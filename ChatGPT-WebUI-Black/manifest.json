{

  "manifest_version": 2,
  "name": "ChatGPT-WebUI-Black",
  "version": "1.3",

  "description": "Black theme for ChatGPT WebUI",

  "icons": {
    
  },

  "content_scripts": [
    {
      "matches": ["*://chat.openai.com/chat", "*://chat.openai.com/chat/*"],
      "js": ["GPTChat_Black.js"]
    },
	{
      "matches": ["*://chat.openai.com/auth/login"],
      "js": ["GPTChat_BlackLogin.js"]
    }
  ]

}
